{
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "semi": true,
  "importOrder": [
    "react",
    "^next$",
    "^next(/.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": true
}
